{
    "name": "myapp/synf-project",
    "description": "Projet Symfony - Site de r√©servation",
    "type": "project",
    "require": {
        "symfony/flex": "^2.8",
        "symfony/framework-bundle": "^7.3",
        "symfony/runtime": "^7.3",
        "symfony/webapp-pack": "*",
        "symfony/yaml": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        }
    }
}
